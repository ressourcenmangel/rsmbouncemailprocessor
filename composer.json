{
	"name": "ressourcenmangel/rsmbouncemailprocessor",
	"type": "typo3-cms-extension",
	"description": "A TYPO3 extension that handles bounce mails in combination with cute_mailing.",
	"homepage": "https://www.ressourcenmangel.de/",
	"authors": [
		{
			"name": "Ralph Brugger",
			"email": "ralph.brugger@ressourcenmangel.de",
			"homepage": "https://www.ressourcenmangel.de/",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"email",
		"newsletter",
		"cute_mailing",
		"TYPO3"
	],
	"support": {
		"issues": ""
	},
	"repositories": [
		{
			"type": "vcs",
			"url": ""
		}
	],
	"require": {
		"ext-json": "*",
		"php": "^8.1",
		"typo3/cms-backend": "^11.5",
		"typo3/cms-core": "^11.5",
		"typo3/cms-extbase": "^11.5",
		"undkonsorten/typo3-cute-mailing": "^4.0"
	},
	"suggest": {
		"undkonsorten/typo3-cute-mailing-registeraddress": "Connector for recipient list if register/unregister subscriber with registeraddress extension.",
		"undkonsorten/typo3-cute-mailing-ttaddress": "tt_address recipient connector. If just tt_address data should be used in recipient list."
	},
	"autoload": {
		"psr-4": {
			"RSM\\rsmbouncemailprocessor\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"RSM\\rsmbouncemailprocessor\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "rsmbouncemailprocessor"
		},
		"branch-alias":{
			"dev-main": "1.0.x-dev"
		}
	}
}
